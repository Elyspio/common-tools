{
	"name": "common-tools",
	"description": "electron+create-react-app sample app",
	"version": "1.0.0",
	"private": true,
	"author": "Elyspio",
	"license": "MIT",
	"homepage": "./",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Elyspio/common-tools"
	},
	"main": "public/electron.js",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
		"prod": "build --em.main=build/electron.js -wl",
		"release": "build --em.main=build/electron.js --publish=always",
		"preprod": "npm run build",
		"prerelease": "npm run build"
	},
	"dependencies": {
		"axios": "^0.19.0",
		"chart.js": "^2.8.0",
		"classnames": "^2.2.6",
		"color": "^3.1.2",
		"cross-env": "^5.1.3",
		"electron": "^4.1.4",
		"electron-is-dev": "^1.1.0",
		"electron-log": "^2.2.14",
		"electron-updater": "^2.19.1",
		"node-notifier": "^5.2.1",
		"primeicons": "^1.0.0",
		"primereact": "^3.1.8",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-chartjs-2": "^2.7.6",
		"react-dom": "^16.9.0",
		"react-redux": "^7.1.0",
		"react-transition-group": "^4.2.2",
		"redux": "^4.0.4",
		"redux-logger": "^3.0.6",
		"systeminformation": "^4.1.5",
		"typescript": "^3.5.3",
		"xml-js": "^1.6.11"
	},
	"devDependencies": {
		"@types/node": "^12.7.2",
		"@types/react": "^16.9.2",
		"@types/react-redux": "^7.1.2",
		"@types/systeminformation": "^3.23.1",
		"concurrently": "^4.1.0",
		"electron-builder": "^19.54.0",
		"prettier": "^1.10.2",
		"react-scripts": "^3.0.1",
		"wait-on": "^3.2.0"
	},
	"build": {
		"appId": "Common tools",
		"files": [
			"build/**/*",
			"node_modules/**/*",
			"package.json"
		],
		"directories": {
			"buildResources": "assets"
		},
		"publish": {
			"provider": "github"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
